# Agent Builder Environment Variables

# Frontend Configuration
VITE_API_BASE_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod
VITE_APP_NAME=Agent Builder
VITE_APP_VERSION=1.0.0
VITE_AWS_REGION=us-east-1
VITE_USER_POOL_ID=us-east-1_xxxxxxxxx
VITE_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Backend Configuration (for local development)
NODE_ENV=development
AWS_REGION=us-east-1
PROJECTS_TABLE=agent-builder-projects-dev
TASKS_TABLE=agent-builder-tasks-dev
ARTIFACTS_TABLE=agent-builder-artifacts-dev
ARTIFACTS_BUCKET=agent-builder-artifacts-dev-123456789
OPENAI_API_KEY=your-openai-api-key-here

# Database Configuration
DYNAMODB_ENDPOINT=http://localhost:8000  # For local DynamoDB
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# Monitoring and Logging
LOG_LEVEL=info
SENTRY_DSN=your-sentry-dsn-here

# Feature Flags
ENABLE_REAL_AI_AGENTS=false  # Set to true when OpenAI integration is ready
ENABLE_WEBSOCKETS=true
ENABLE_ANALYTICS=true